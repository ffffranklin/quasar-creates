{
  "name": "quasar-creates",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "pnpm test:unit",
    "test:unit": "vitest -c ./vitest.config.unit.ts",
    "test:unit:ui": "vitest -c ./vitest.config.unit.ts --ui",
    "prisma": "eval $(grep '^DATABASE_URL' .env) prisma",
    "prisma:generate": "pnpm run prisma generate",
    "prisma:migrate:dev": "pnpm run prisma migrate dev",
    "prisma:migrate:reset": "pnpm run prisma migrate reset",
    "prisma:db:push": "pnpm run prisma db push",
    "prisma:db:seed": "pnpm run prisma db seed"
  },
  "engines": {
    "node": "^24.0.0",
    "pnpm": "^10.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "prisma": "^6.18.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.3",
    "vitest-mock-extended": "^3.1.0"
  }
}
